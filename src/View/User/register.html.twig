{% extends 'layout.html.twig' %}

{% block content %}

	<div class="row mt-5 mb-5">
		<div class="col-md-8 offset-md-2">
			<div class=" p-3 mb-4" role="alert" aria-live="polite" aria-labelledby="alert-message">
				<ul class="bg-warning list-unstyled p-0">
					{% for error in errors %}
						<li>
							{{ error }}
						</li>
					{% endfor %}
				</ul>
			</div>

			{% include 'User/_form-register.html.twig' with {'formId': 'form-register-page'} %}
		</div>
	</div>
{% endblock %}
